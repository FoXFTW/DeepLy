{
  "name": "octfx/deeply",
  "version": "3.0.1",
  "description": "DeepLy is a PHP client for the DeepL.com translation API",
  "keywords": [
    "deepl",
    "deeply",
    "translation",
    "translate",
    "language",
    "api",
    "client",
    "english",
    "german",
    "french"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Konnertz"
    },
    {
      "name": "Octfx"
    }
  ],
  "require": {
    "ext-mbstring": "*",
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7"
  },
  "autoload": {
    "psr-4": {
      "Octfx\\DeepLy\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Octfx\\DeepLy\\Integrations\\Laravel\\DeepLyServiceProvider"
      ],
      "aliases": {
        "DeepLy": "Octfx\\DeepLy\\Integrations\\Laravel\\DeepLyFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
